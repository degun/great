---
layout: layouts/base.njk
section: project
---

{% from "components/pagetitle.njk" import pageTitle %}

{{pageTitle(title)}}

<article class="single-project" id="content">

  {{ layoutContent | safe }}

  <div class="images">
    {% for image in images %}
      
      {% img image | replace("/static", "./static"), "page-image", "" + title + "", "", [400], "animation-delay: " + loop.index0 * 100 + "ms" %}
    {% endfor %}
  </div>
  
</article>
